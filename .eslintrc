{
  "extends": "airbnb",
  "parser": "babel-ESlint",
  "rules": {
      "one-var": 0,
      "complexity": [2, 3],
      "id-length": 0,
      "func-names": 0,
      "indent": [2, 2, {"SwitchCase": 1}],
      "quotes": [2, "single"],
      "linebreak-style": [2, "unix"],
      "semi": [2, "always"],
      "strict": 0,
      "no-console": 0,
      "prefer-const": 0,
      "comma-dangle": 0,
      "react/prefer-es6-class": [1, "never"],
      "react/react-in-jsx-scope": 1,
      "no-use-before-define": [1, "nofunc"],
      "jsx-quotes": 0

  },
  "env": {
      "es6": true,
      "node": true,
      "browser": true
  },
  "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
  },
  "plugins": ["react"]
}